<?xml version="1.0" encoding="UTF-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <gradient android:startColor="@color/cpb_blue" android:endColor="@color/cpb_blue" android:angle="270.0" />
            <corners android:topLeftRadius="0.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="0.0dip" android:bottomRightRadius="0.0dip" />
            <stroke android:width="1.0px" android:color="@color/cpb_blue" />
        </shape>
    </item>
    <item android:state_focused="true" android:state_enabled="true">
        <shape>
            <gradient android:startColor="@color/cpb_blue" android:endColor="@color/cpb_blue" android:angle="270.0" />
            <corners android:topLeftRadius="0.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="0.0dip" android:bottomRightRadius="0.0dip" />
            <stroke android:width="1.0px" android:color="@color/cpb_blue" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ffff0000" android:endColor="#ffff0000" android:angle="270.0" />
            <corners android:topLeftRadius="0.0dip" android:topRightRadius="0.0dip" android:bottomLeftRadius="0.0dip" android:bottomRightRadius="0.0dip" />
            <stroke android:width="1.0px" android:color="#FF0000" />
        </shape>
    </item>
</selector>