<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/proxyLayoutMaster"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <include
        android:id="@+id/topLayout"
        layout="@layout/layout_top_layout_other"
        android:layout_width="fill_parent"
        android:layout_height="90dp"
        android:layout_alignParentTop="true" />

    <RelativeLayout
        android:id="@+id/benefProxyHeadingLayout"
        android:layout_below="@id/topLayout"
        android:background="@color/grey"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <TextView
            android:id="@+id/benefDetailHeading"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="25dp"
            android:textColor="@color/white"
            android:text=""/>
        <TextView
            android:id="@+id/proxyDetailHeading"
            android:layout_marginLeft="540dp"
            android:layout_marginRight="50dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="25dp"
            android:textColor="@color/white"
            android:text=""/>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/secondLayout"
        android:layout_below="@id/benefProxyHeadingLayout"
        android:layout_above="@id/bottom_layout"
        android:orientation="horizontal"
        android:background="@color/grey"
        android:weightSum="2"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/cardHolderDetailsLayout"
            android:layout_weight="1"
            android:background="@color/white"
            android:orientation="vertical"
            android:padding="10dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/rcNumberTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25dp"
                android:textColor="#0288D1"
                android:text=""/>
            <TextView
                android:id="@+id/rcNumberTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25dp"
                android:textColor="@color/grey"
                android:text=""/>
            <TextView
                android:id="@+id/holderName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25dp"
                android:textColor="#0288D1"
                android:text=""/>
            <TextView
                android:id="@+id/holderNameTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25dp"
                android:textColor="@color/grey"
                android:text=""/>
            <TextView
                android:id="@+id/holderAge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25dp"
                android:textColor="#0288D1"
                android:text=""/>
            <TextView
                android:id="@+id/holderAgeTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25dp"
                android:textColor="@color/grey"
                android:text=""/>
            <LinearLayout
                android:id="@+id/childAdultCylLayout"
                android:layout_marginTop="5dp"
                android:background="@color/grey"
                android:padding="2dp"
                android:orientation="horizontal"
                android:weightSum="6"
                android:layout_width="match_parent"
                android:layout_height="60dp">
                    <LinearLayout
                        android:background="@color/grey"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:src="@drawable/icon_adults"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:background="@color/white"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <TextView
                            android:id="@+id/adultCountTv"
                            android:text=""
                            android:textSize="25dp"
                            android:textColor="@color/grey"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center"
                        android:layout_weight="1"
                        android:background="@color/grey"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:src="@drawable/icon_children"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:background="@color/white"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <TextView
                            android:id="@+id/childCountTv"
                            android:text=""
                            android:textSize="25dp"
                            android:textColor="@color/grey"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center"
                        android:layout_weight="1"
                        android:background="@color/grey"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:src="@drawable/icon_cylinder"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:background="@color/white"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <TextView
                            android:id="@+id/cylinderCountTv"
                            android:text=""
                            android:textSize="25dp"
                            android:textColor="@color/grey"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/mobileAadharLayout"
                android:layout_marginTop="7dp"
                android:background="@color/grey"
                android:padding="2dp"
                android:orientation="horizontal"
                android:weightSum="4"
                android:layout_width="match_parent"
                android:layout_height="60dp">
                    <LinearLayout
                        android:gravity="center"
                        android:background="@color/grey"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:src="@drawable/card_holder_mobile"
                            android:layout_width="60dp"
                            android:layout_height="60dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:background="@color/white"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <TextView
                            android:id="@+id/mobileTv"
                            android:text=""
                            android:textSize="25dp"
                            android:textColor="@color/grey"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                    <LinearLayout
                        android:gravity="center"
                        android:layout_weight="1"
                        android:background="@color/grey"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <ImageView
                            android:src="@drawable/card_holder_aadhar"
                            android:layout_width="60dp"
                            android:layout_height="60dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:background="@color/white"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <TextView
                            android:id="@+id/aadharTv"
                            android:text=""
                            android:textSize="25dp"
                            android:textColor="@color/grey"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/proxyListLayout"
            android:layout_marginLeft="1dp"
            android:background="@color/white"
            android:orientation="vertical"
            android:layout_weight="1"
            android:padding="10dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ListView
                android:id="@+id/proxyList"
                android:layout_width="match_parent"
                android:layout_height="275dp"
                android:background="@color/white"
                android:divider="#777777"
                android:dividerHeight="1dp"
                android:listSelector="#efefef">
            </ListView>
            <LinearLayout
                android:id="@+id/addProxyLayout"
                android:layout_below="@id/proxyList"
                android:background="#0288D1"
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="60dp">
                <ImageView
                    android:src="@drawable/proxy_member_icon"
                    android:layout_width="50dp"
                    android:layout_height="50dp" />
                <TextView
                    android:id="@+id/addProxyTv"
                    android:text=""
                    android:textSize="25dp"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <include
        android:id="@+id/bottom_layout"
        layout="@layout/user_bottom_other_screen"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" />

</RelativeLayout>
