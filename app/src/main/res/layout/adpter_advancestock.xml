<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:orientation="vertical">
    <View
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:background="#777777"/>

    <LinearLayout
        android:id="@+id/linearLayoutTitleAdapter"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/commodityBackground"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:weightSum="1">
            <View
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="#777777"/>
            <TextView
                android:id="@+id/sno"
                android:layout_width="50dp"
                android:layout_height="fill_parent"
                android:layout_marginLeft="5dp"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@color/grey"
                android:textSize="16sp"
                android:layout_weight="3.17" />


        </LinearLayout>

        <RelativeLayout
            android:id="@+id/challanIdLay"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center">
            <View
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="#777777"/>

            <TextView
                android:id="@+id/challanIdTv"
                android:layout_width="100dp"
                android:layout_height="fill_parent"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@color/grey"
                android:textSize="16sp"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/dispatchDateLay"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center">
            <View
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="#777777"/>

            <TextView
                android:id="@+id/dispatchDateTv"
                android:layout_width="100dp"
                android:layout_height="fill_parent"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@color/grey"
                android:textSize="16sp"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/godownCodeLay"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center">
            <View
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="#777777"/>

            <TextView
                android:id="@+id/godownCodeTv"
                android:layout_width="130dp"
                android:layout_height="fill_parent"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@color/grey"
                android:textSize="16sp"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/unitEntitlement"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_gravity="center">
            <View
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="#777777"/>

            <TextView
                android:id="@+id/producttv"
                android:layout_width="190dp"
                android:layout_height="fill_parent"
                android:gravity="center"
                android:textColor="@color/grey"
                android:textSize="16sp"
                />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/unitLayout"
            android:layout_width="50dp"
            android:layout_height="fill_parent">
            <View
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="#777777"/>
             <TextView
                android:id="@+id/fpsInvardDetailUnitId"
                android:layout_width="50dp"
                android:layout_height="fill_parent"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="Unit"
                android:textSize="@dimen/table_title"

                />
            </RelativeLayout>


        <LinearLayout
            android:layout_width="100dp"
            android:layout_height="fill_parent"
            android:weightSum="1"
            android:orientation="horizontal"
            android:id="@+id/lin">
            <View
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="#777777"/>

            <TextView
                android:id="@+id/monthtv"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:gravity="center"
                android:textColor="@color/grey"
                android:textSize="16sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="80dp"
            android:id="@+id/stat"
            android:layout_height="fill_parent">
            <View
                android:layout_width="2dp"
                android:layout_height="fill_parent"
                android:background="#777777"/>

            <TextView
                android:id="@+id/yeartv"
                android:layout_width="80dp"
                android:layout_height="fill_parent"
                android:gravity="center"
                android:textColor="@color/grey"
                android:textSize="16sp" />

        </LinearLayout>
        <View
            android:layout_width="2dp"
            android:layout_height="fill_parent"
            android:background="#777777"/>

        <LinearLayout
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:id="@+id/viewmo"
            >

            <TextView
                android:id="@+id/quantitytv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textColorHint="#c77000"
                android:textSize="16sp" />


        </LinearLayout>

        <View
            android:layout_width="2dp"
            android:layout_height="fill_parent"
            android:background="#777777"/>

        <LinearLayout
            android:layout_width="45dp"
            android:layout_height="match_parent"
            android:id="@+id/processTvLayout"
            >
            <TextView
                android:id="@+id/processTv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textColorHint="#c77000"
                android:textSize="16sp" />
        </LinearLayout>

        <View
            android:layout_width="2dp"
            android:layout_height="fill_parent"
            android:background="#777777"/>


        <LinearLayout
            android:layout_width="30dp"
            android:layout_height="match_parent"
            android:id="@+id/syncTvLayout"
            >
            <TextView
                android:id="@+id/syncTv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textColorHint="#c77000"
                android:textSize="16sp" />
        </LinearLayout>




    </LinearLayout>


</LinearLayout>
