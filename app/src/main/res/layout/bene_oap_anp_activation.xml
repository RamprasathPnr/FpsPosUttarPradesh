<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:pixlui="http://schemas.android.com/apk/com.neopixl.pixlui"
    android:id="@+id/bill_layout_master"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <include
        android:id="@+id/topLayout"
        layout="@layout/layout_top_layout_other"
        android:layout_width="fill_parent"
        android:layout_height="90dp"
        android:layout_alignParentTop="true" />

    <LinearLayout
        android:id="@+id/userLayout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@+id/bottom_layout"
        android:layout_below="@+id/topLayout"
        android:layout_margin="10dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/rationCardTypeLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="15dp"
                    android:text="@string/cardCap"
                    android:textColor="#777777"
                    android:textSize="23dp" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/back_ground_otp_register1"
                    android:orientation="horizontal">

                    <!--<RelativeLayout
                        android:layout_width="50dp"
                        android:layout_height="fill_parent"
                        android:background="@color/card_activate">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:padding="10dp"
                            android:src="@drawable/icon_card_type" />
                    </RelativeLayout>-->


                    <RelativeLayout
                        android:id="@+id/cardTypeSpinnerLayout"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent">

                        <com.omneagate.Util.NoDefaultSpinner
                            android:id="@+id/rationCardTypeSpinner"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/transparent" />

                        <RelativeLayout
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_alignParentRight="true">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:padding="10dp"
                                android:src="@drawable/arrow_brown" />
                        </RelativeLayout>
                    </RelativeLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/aRegisterNoLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="15dp"
                    android:text="@string/aRegisterNo"
                    android:textColor="#777777"
                    android:textSize="23dp" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/enterOtp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/back_ground_otp_register1"
                    android:paddingLeft="10dp">


                    <com.neopixl.pixlui.components.edittext.EditText
                        android:id="@+id/aRegisterNoEt"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="#00000000"
                        android:digits="0123456789"
                        android:ems="10"
                        android:gravity="center_vertical"
                        android:inputType="numberDecimal"
                        android:maxLength="3"
                        android:singleLine="true"
                        android:textColor="#444444"
                        android:textSize="33dp"
                        pixlui:clearclipboardcontent="true"
                        pixlui:copyandpaste="false" />

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/rationCardNoLabel"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="15dp"
                    android:text="@string/ration_card_number"
                    android:textColor="#777777"
                    android:textSize="23dp" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/back_ground_otp_register1"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/firstText"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:gravity="center"
                        android:ems="2"
                        android:text="  "
                        android:textColor="#444444"
                        android:textSize="30dp" />

                    <View
                        android:layout_width="1dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="15dp"
                        android:background="#777777" />

                    <TextView
                        android:id="@+id/secondText"
                        android:ems="1"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:gravity="center"
                        android:text=" "
                        android:textColor="#444444"
                        android:textSize="30dp" />


                    <View
                        android:layout_width="1dp"
                        android:layout_height="fill_parent"
                        android:background="#777777" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:paddingLeft="10dp">

                        <com.neopixl.pixlui.components.edittext.EditText
                            android:id="@+id/thirdText"
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:background="#00000000"
                            android:digits="0123456789"
                            android:ems="7"
                            android:gravity="center_vertical"
                            android:inputType="numberDecimal"
                            android:maxLength="7"
                            android:singleLine="true"
                            android:textColor="#444444"
                            android:textSize="33dp"
                            pixlui:clearclipboardcontent="true"
                            pixlui:copyandpaste="false" />
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/mobileNoLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="15dp"
                    android:text="@string/mob_number"
                    android:textColor="#777777"
                    android:textSize="23dp" />

                <LinearLayout
                    android:id="@+id/layout_otp"
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_below="@+id/enterOtp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/back_ground_otp_register1"
                    android:paddingLeft="10dp">

                    <TextView
                        android:id="@+id/mobileNumberPrefix"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:gravity="center_vertical"
                        android:text="+91"
                        android:textColor="#444444"
                        android:textSize="30dp" />

                    <View
                        android:layout_width="1dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="15dp"
                        android:background="#777777" />

                    <com.neopixl.pixlui.components.edittext.EditText
                        android:id="@+id/mobileNumberActivationEt"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:background="#00000000"
                        android:digits="0123456789"
                        android:ems="10"
                        android:gravity="center_vertical"
                        android:inputType="numberDecimal"
                        android:maxLength="10"
                        android:singleLine="true"
                        android:textColor="#444444"
                        android:textSize="33dp"
                        pixlui:clearclipboardcontent="true"
                        pixlui:copyandpaste="false" />

                </LinearLayout>



            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/aadharCardNoLabel"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="15dp"
                    android:text="@string/aadharNo"
                    android:textColor="#777777"
                    android:textSize="23dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="50dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/back_ground_otp_register1"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="170dp"
                        android:layout_height="fill_parent">

                        <TextView
                            android:id="@+id/firstAadharText"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="#00000000"
                            android:digits="0123456789"
                            android:ems="4"
                            android:gravity="center"
                            android:inputType="number"
                            android:maxLength="4"
                            android:nextFocusDown="@+id/thirdAadharText"
                            android:singleLine="true"
                            android:textColor="#444444"
                            android:textSize="33dp"/>
                    </LinearLayout>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="fill_parent"
                        android:background="#777777" />

                    <LinearLayout
                        android:layout_width="170dp"
                        android:layout_height="fill_parent">

                        <TextView
                            android:id="@+id/secondAadharText"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="#00000000"
                            android:digits="0123456789"
                            android:ems="4"
                            android:gravity="center"
                            android:inputType="number"
                            android:maxLength="4"
                            android:nextFocusDown="@+id/thirdAadharText"
                            android:singleLine="true"
                            android:textColor="#444444"
                            android:textSize="33dp"/>
                    </LinearLayout>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="fill_parent"
                        android:background="#777777" />

                    <LinearLayout
                        android:layout_width="170dp"
                        android:layout_height="fill_parent"
                        android:paddingLeft="10dp">

                        <TextView
                            android:id="@+id/thirdAadharText"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="#00000000"
                            android:digits="0123456789"
                            android:ems="4"
                            android:gravity="center"
                            android:inputType="number"
                            android:maxLength="4"
                            android:nextFocusDown="@+id/thirdAadharText"
                            android:singleLine="true"
                            android:textColor="#444444"
                            android:textSize="33dp"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <!--<LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/dummyLabelOne"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="15dp"
                    android:text=""
                    android:textColor="#777777"
                    android:textSize="23dp" />

                <LinearLayout
                    android:visibility="invisible"
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/back_ground_otp_register1"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="50dp"
                        android:layout_height="fill_parent"
                        android:background="@color/card_activate">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_margin="10dp"
                            android:src="@drawable/icon_children" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent">

                        <com.omneagate.Util.NoDefaultSpinner
                            android:id="@+id/dummySpinnerOne"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/transparent" />

                        <RelativeLayout
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_alignParentRight="true">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:padding="10dp"
                                android:src="@drawable/arrow_brown" />
                        </RelativeLayout>
                    </RelativeLayout>

                </LinearLayout>
            </LinearLayout>-->

                <ImageView
                    android:id="@+id/scanAadhar"
                    android:src="@drawable/img_qr_code"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="37dp"
                    android:layout_marginRight="390dp"/>

            </RelativeLayout>


        </LinearLayout>



        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <Button
                android:textAllCaps="false"
                android:id="@+id/cancel_button"
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="20dp"
                android:layout_toLeftOf="@+id/submit_button"
                android:background="#dd0632"
                android:text="Cancel"
                android:textColor="@color/white"
                android:textSize="25dp" />

            <Button
                android:textAllCaps="false"
                android:id="@+id/submit_button"
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:layout_gravity="left"
                android:layout_marginRight="20dp"
                android:layout_marginTop="20dp"
                android:background="#029555"
                android:text="Submit"
                android:textColor="@color/white"
                android:textSize="25dp" />
        </RelativeLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/key_board_custom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_margin="30dp"
        android:background="@drawable/edit_card_registartion"
        android:padding="10dp">

        <RelativeLayout
            android:id="@+id/keyboardNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <android.inputmethodservice.KeyboardView
                android:id="@+id/customkeyboard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="bottom"
                android:background="@color/transparent"
                android:keyBackground="@color/keyboard_key"
                android:visibility="gone"

                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/keyboardAlpha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone">

            <android.inputmethodservice.KeyboardView
                android:id="@+id/customkeyboardAlpha"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_gravity="bottom"
                android:background="@color/transparent"
                android:keyBackground="@color/keyboard_key"
                android:visibility="gone"

                />
        </RelativeLayout>
    </RelativeLayout>


    <include
        android:id="@+id/bottom_layout"
        layout="@layout/user_bottom_other_screen"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" />

   <!-- <FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/cameraPreview"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>-->

</RelativeLayout>
